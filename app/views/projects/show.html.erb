<div class="page-header">
  <%= link_to projects_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_project_path(@project), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show project</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @project.name %></dd>

  <dt>Description:</dt>
  <dd><%= @project.description %></dd>

  <dt>Partners:</dt>
  <dd><%= @project.partners %></dd>

  <dt>Country:</dt>
  <dd><%= @project.country_id %></dd>

  <dt>Projectstatus:</dt>
  <dd><%= @project.projectstatus_id %></dd>

  <dt>Publish:</dt>
  <dd><%= @project.publish %></dd>

  <dt>Project start:</dt>
  <dd><%= @project.project_start %></dd>

  <dt>Project end:</dt>
  <dd><%= @project.project_end %></dd>

</dl>
