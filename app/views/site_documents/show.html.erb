<div class="page-header">
  <%= link_to site_documents_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_site_document_path(@site_document), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show site_document</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @site_document.name %></dd>

  <dt>Documentable Type:</dt>
  <dd><%= @site_document.documentable_type %></dd>

  <dt>Documentable:</dt>
  <dd><%= @site_document.documentable_id %></dd>

</dl>
