<div class="page-header">
  <%= link_to site_images_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_site_image_path(@site_image), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show site_image</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @site_image.name %></dd>

  <dt>:</dt>
  <dd><%= @site_image.imageable_type %></dd>

  <dt>Imageable:</dt>
  <dd><%= @site_image.imageable_id %></dd>


  <%= image_tag @site_image.image.url %>
  <%= image_tag @site_image.image.url(:medium) %>
  <%= image_tag @site_image.image.url(:thumb) %>

























</dl>
