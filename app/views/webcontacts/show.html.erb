<div class="page-header">
  <%= link_to webcontacts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_webcontact_path(@webcontact), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show webcontact</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @webcontact.name %></dd>

  <dt>Email:</dt>
  <dd><%= @webcontact.email %></dd>

  <dt>Subject:</dt>
  <dd><%= @webcontact.subject %></dd>

  <dt>Message:</dt>
  <dd><%= @webcontact.message %></dd>

  <dt>Recorded:</dt>
  <dd><%= @webcontact.recorded %></dd>

</dl>
